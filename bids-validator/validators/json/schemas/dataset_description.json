{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.com/example.json",
  "properties": {
    "Name": {
      "type": "string"
    },
    "BIDSVersion": {
      "type": "string"
    },
    "License": {
      "type": "string"
    },
    "Authors": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "Acknowledgements": {
      "items": {
        "type": "string"
      },
      "type": "string"
    },
    "HowToAcknowledge": {
      "items": {
        "type": "string"
      },
      "type": "string"
    },
    "Funding": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "ReferencesAndLinks": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
